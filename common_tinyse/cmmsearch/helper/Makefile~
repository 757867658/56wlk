libtshelper.a : CPPFLAGS := -DVERSION=\"1.0.0.0\" -g -finline-functions -Wall -Winline -pipe -DHASH_THREAD_SAFE -D_XOPEN_SOURE=500 -D_GNU_SOURCE
libtshelper.a : IDLFLAGS := 
libtshelper.a : CFLAGS := -pipe -Wall -W -g -fPIC
libtshelper.a : INCPATH :=  -I. -I../core -I../lib/utils -I../include $(DEP_INCPATH)
libtshelper.a : LDLIBS := -lcrypto -lm -lpthread -lpthread -lcrypto -lrt -rdynamic $(DEP_LDLIBS)
libtshelper.a : LDFLAGS :=  $(DEP_LDFLAGS)
libtshelper.a : LINKFLAGS := -rdynamic

libtshelper_a_SOURCES := $(wildcard pgmake_autogen.cpp ./data_accessor.cpp ./default.cpp)
#libtshelper_a_HEADERS := $(wildcard *.h *.hpp)
libtshelper_a_HEADERS := *.h *.hpp
libtshelper_a_IDLSRC := $(wildcard __$IDLSRC_VALUE$__)
libtshelper_a_IDLSRCCPP := $(patsubst %.idl,%.cpp, $(libtshelper_a_IDLSRC))
libtshelper_a_IDLSRCH := $(patsubst %.idl,%.h, $(libtshelper_a_IDLSRC))
libtshelper_a_CONFDES := $(wildcard __$CONFDES$_VALUE$__ libtshelper.a.des)
libtshelper_a_CONFRANGE := $(patsubst %.des,%.range, $(libtshelper_a_CONFDES))
libtshelper_a_OBJS := $(libtshelper_a_IDLSRC:.idl=.o)
libtshelper_a_OBJS += $(libtshelper_a_SOURCES:.c=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.cc=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.cpp=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.cxx=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.C=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.idl=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.yacc=.o)
libtshelper_a_OBJS := $(libtshelper_a_OBJS:.lex=.o)
libtshelper.a : $(libtshelper_a_OBJS) 
	rm -f $@ || echo 
	ar cr $@ $^


define libtshelper_a_output
	mkdir -p output
	-mkdir -p output/lib
	-cp -rf libtshelper.a output/lib
	-mkdir -p output/include
	-cp -rf $(libtshelper_a_HEADERS) output/include
	`if [ -e "conf" ];then cp -rf conf output/ ;fi`

endef
	

define libtshelper_a_clean
	-rm -rf $(libtshelper_a_OBJS) $(libtshelper_a_IDLSRCCPP) $(libtshelper_a_IDLSRCH) libtshelper.a.range conf/libtshelper.a.range
	-rm -rf libtshelper.a
	-rm -rf output
	-rm -rf ccp_output.error   ccp_output_scm.xml  ccp_output.xml ccp_output.pclint  ccp_output.txt
endef
	

define libtshelper_a_ccpc
	ccp $(libtshelper_a_SOURCES) --formatter vim
endef
	

define libtshelper_a_cov
	
endef
	



